{
    "vim.handleKeys": {
        "<C-c>": false,
        "<C-v>": false,
        "<C-a>": false,
        "<C-x>": false,
        "<C-b>": false
    },
    "vim.easymotion": true,
    "vim.leader": "<Space>",
    "vim.sneak": true,
    "vim.normalModeKeyBindings": [
        {
            "before": ["H"],
            "after": ["^"]
        },
        {
            "before": ["L"],
            "after": ["g","_"]
        },
        {
            "before": ["J"],
            "after": ["5","j"]
        },
        {
            "before": ["K"],
            "after": ["5","k"]
        },
        // 删除函数
        {
            "before": ["<leader>","d","f"],
            "after": ["V","$","%","d"]
        } 
    ],
    "vim.insertModeKeyBindings": [
        {
            "before":["ctrl+h"],
            "after": ["Left"]
        },
        {
            "before":["ctrl+l"],
            "after": ["Right"]
        },
        {
            "before":["ctrl+j"],
            "after": ["Down"]
        },
        {
            "before":["ctrl+k"],
            "after": ["Up"]
        }
    ],
    "vim.operatorPendingModeKeyBindings": [
        {
            "before": ["H"],
            "after": ["^"]
        },
        {
            "before": ["L"],
            "after": ["g","_"]
        },
        // 选中函数
        {
            "before": ["a","i"],
            "after": ["a","I"]
        }
        
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["J"],
            "after": ["5","j"]
        },
        {
            "before": ["K"],
            "after": ["5","k"]
        },
        // 选中函数
        {
            "before": ["a","i"],
            "after": ["a","I"]
        }
    ],
     "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["f"],
            "after": ["s"]
        },{
            "before": ["F"],
            "after": ["S"]
        },{
            "before": ["s"],
            "after": ["c","l"]
        },{
            "before": ["S"],
            "after": ["^","C"]
        }
    ],
    "vim.operatorPendingModeKeyBindingsNonRecursive": [
        {
            "before": ["f"],
            "after": ["z"]
        },{
            "before": ["F"],
            "after": ["Z"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
         {
            "before": ["f"],
            "after": ["s"]
        }
    ],
}